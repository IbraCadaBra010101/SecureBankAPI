{
  "name": "fastighets-app",
  "version": "1.0.0",
  "description": "Real Estate Management Application with .NET Backend and React Frontend",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd FastighetsApp && dotnet run",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "real-estate",
    "dotnet",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}
